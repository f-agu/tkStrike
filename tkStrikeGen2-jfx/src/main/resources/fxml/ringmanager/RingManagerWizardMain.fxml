<BorderPane fx:id="rootView" maxWidth="900.0" minHeight="550.0" minWidth="900.0" prefWidth="900.0" styleClass="tkStrike-window" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.xtremis.daedo.tkstrike.ui.controller.ringmanager.RingManagerWizardController">
   <right>

   </right>
   <padding>
      <Insets bottom="10.0" left="15.0" right="15.0" top="10.0" />
   </padding>
   <bottom>
      <HBox alignment="CENTER" spacing="15.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btCancel" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#doCancel" text="%button.cancel">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <styleClass>
                  <String fx:value="button-window-reverse" />
                  <String fx:value="wz-button-control" />
                  <String fx:value="button-image-cancel-reverse" />
               </styleClass>
            </Button>
            <Button fx:id="btPrevious" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#doPrevious" text="%button.previous">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <styleClass>
                  <String fx:value="button-window" />
                  <String fx:value="wz-button-control" />
                  <String fx:value="button-image-prev" />
               </styleClass></Button>
            <Button fx:id="btNext" mnemonicParsing="false" onAction="#doNext" text="%button.next">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <styleClass>
                  <String fx:value="button-window" />
                  <String fx:value="wz-button-control" />
                  <String fx:value="button-image-next" />
               </styleClass></Button>
            <Button fx:id="btFinish" layoutX="111.0" layoutY="10.0" mnemonicParsing="false" onAction="#doFinish" text="%button.finish">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <styleClass>
                  <String fx:value="button-window-reverse" />
                  <String fx:value="wz-button-control" />
                  <String fx:value="button-image-save" />
               </styleClass></Button>
         </children>
         <padding>
            <Insets top="5.0" />
         </padding>
      </HBox>
   </bottom>
   <center>
      <AnchorPane fx:id="pnMain" maxHeight="430.0" minHeight="430.0" prefHeight="430.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </BorderPane.margin></AnchorPane>
   </center>
   <top>
      <HBox alignment="TOP_CENTER" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="lblMatchNumber" styleClass="wz-breadcrumb-label" />
            <Label fx:id="lblPhase" layoutX="238.0" layoutY="10.0" styleClass="wz-breadcrumb-label" />
            <Label fx:id="lblSubCategory" layoutX="243.0" layoutY="10.0" styleClass="wz-breadcrumb-label" />
            <Label fx:id="lblGender" layoutX="248.0" layoutY="10.0" styleClass="wz-breadcrumb-label" />
            <Label fx:id="lblCategory" layoutX="253.0" layoutY="10.0" styleClass="wz-breadcrumb-label" />
         </children>
      </HBox>
   </top>
</BorderPane>
